{
    "name": "tipoff/seo",
    "description": "Laravel Package for tracking SEO keywords",
    "keywords": [
        "tipoff",
        "seo"
    ],
    "homepage": "https://github.com/tipoff/seo",
    "license": "MIT",
    "authors": [
        {
            "name": "Tipoff",
            "email": "packages@tipoff.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "drewroberts/media": "^3.2.2",
        "tipoff/addresses": "^2.0.0",
        "tipoff/authorization": "^2.3.1",
        "tipoff/laravel-google-api": "^1.3.0",
        "tipoff/laravel-serpapi": "^1.0.4",
        "tipoff/support": "^1.5.7"
    },
    "require-dev": {
        "tipoff/test-support": "^1.2.0"
    },
    "autoload": {
        "psr-4": {
            "Tipoff\\Seo\\": "src",
            "Tipoff\\Seo\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tipoff\\Seo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tipoff\\Seo\\SeoServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
